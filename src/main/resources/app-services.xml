<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:aop="http://www.springframework.org/schema/aop"
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
           http://www.springframework.org/schema/beans/spring-beans-4.0.xsd">
    <import resource="classpath*:jbean-service.xml" />
    <import resource="classpath*:jmis-service.xml" />

    <!-- 范例 -->
    <bean id="SvrExample.search" class="cn.cerc.example.service.SvrExample">
        <property name="funcCode" value="search"></property>
    </bean>
    <bean id="SvrExample.append" class="cn.cerc.example.service.SvrExample">
        <property name="funcCode" value="append"></property>
    </bean>
    <bean id="SvrExample.download" class="cn.cerc.example.service.SvrExample">
        <property name="funcCode" value="download"></property>
    </bean>
    <bean id="SvrExample.modify" class="cn.cerc.example.service.SvrExample">
        <property name="funcCode" value="modify"></property>
    </bean>
    <bean id="SvrExample.delete" class="cn.cerc.example.service.SvrExample">
        <property name="funcCode" value="delete"></property>
    </bean>
    <!-- 商品资料 -->
    <bean id="SvrPartInfo.search" class="cn.cerc.example.service.SvrPartInfo">
        <property name="funcCode" value="search"></property>
    </bean>
    <bean id="SvrPartInfo.append" class="cn.cerc.example.service.SvrPartInfo">
        <property name="funcCode" value="append"></property>
    </bean>
    <bean id="SvrPartInfo.download" class="cn.cerc.example.service.SvrPartInfo">
        <property name="funcCode" value="download"></property>
    </bean>
    <bean id="SvrPartInfo.modify" class="cn.cerc.example.service.SvrPartInfo">
        <property name="funcCode" value="modify"></property>
    </bean>
</beans>