<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-4.0.xsd">
    <import resource="classpath*:jbean-service.xml"/>
    <import resource="classpath*:jmis-service.xml"/>

    <!-- 范例 SvrExample.search-->
    <bean id="SvrExample.search" class="cn.cerc.example.service.SvrExample" scope="prototype">
        <property name="funcCode" value="search"></property>
    </bean>
    <bean id="SvrExample.append" class="cn.cerc.example.service.SvrExample" scope="prototype">
        <property name="funcCode" value="append"></property>
    </bean>
    <bean id="SvrExample.download" class="cn.cerc.example.service.SvrExample" scope="prototype">
        <property name="funcCode" value="download"></property>
    </bean>
    <bean id="SvrExample.modify" class="cn.cerc.example.service.SvrExample" scope="prototype">
        <property name="funcCode" value="modify"></property>
    </bean>
    <bean id="SvrExample.delete" class="cn.cerc.example.service.SvrExample" scope="prototype">
        <property name="funcCode" value="delete"></property>
    </bean>
    <bean id="SvrExample.test" class="cn.cerc.example.service.SvrExample" scope="prototype">
        <property name="funcCode" value="test"></property>
    </bean>
</beans>